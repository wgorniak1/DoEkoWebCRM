@{
    DoEko.ViewModels.TestViewModels.ListPhotosViewModel item = ViewBag.Item;
//    SortedList<string, DoEko.ViewModels.TestViewModels.SurveyAttachment> att = ViewBag.Item;
} 
<div class="row">
    <div class="col-sm-2" title="">(string)ViewData["Id"]</div>
    <div class="col-sm-10">
        @if (item.Attachments.Keys.Any(k => k == (string)ViewData["Id"]))
        {
            <a href="@item.Attachments[(string)ViewData["Id"]].Url" target="_blank" style="cursor:pointer">
                @item.Attachments[(string)ViewData["Id"]].FileName
            </a>
        }
        else
        { <span class="bg-danger">brak</span> }
    </div>
</div>
