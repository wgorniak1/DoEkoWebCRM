<environment names="Development">
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.15.0/jquery.validate.js"
            asp-fallback-src="~/lib/jquery-validation/dist/jquery.validate.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator">
    </script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.js"
            asp-fallback-src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
    </script>
    <script src="~/lib/jquery-validation/src/localization/messages_pl.js" asp-append-version="false" type="text/javascript"></script>
    <script src="~/js/CustomValidationWithDataAnnotation.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/ValidationFixes.js" asp-append-version="true" type="text/javascript"></script>

</environment>
<environment names="Staging,Production">
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.15.0/jquery.validate.min.js"
            asp-fallback-src="~/lib/jquery-validation/dist/jquery.validate.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator">
    </script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"
            asp-fallback-src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
    </script>
    <script src="~/lib/jquery-validation/src/localization/messages_pl.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/CustomValidationWithDataAnnotation.min.js" asp-append-version="true" type="text/javascript"></script>
    <script src="~/js/ValidationFixes.js" asp-append-version="true" type="text/javascript"></script>
</environment>
