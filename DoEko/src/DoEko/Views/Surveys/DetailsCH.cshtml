                        </select>
                        <span class="help-block"></span>
                        <span asp-validation-for="InvestmentAddress.DistrictId" class="text-danger" />
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group-sm">
                        <label asp-for="InvestmentAddress.CommuneId" class="control-label"></label>
                        <select asp-for="InvestmentAddress.CommuneId" class="form-control address-commune" data-address-key="InvAddr" asp-items="ViewBag.InvAddrCommuneId">
