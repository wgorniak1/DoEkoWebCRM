@model DoEko.ViewModels.SurveyViewModels.DetailsCHViewModel
@using DoEko.Controllers.Extensions
@using DoEko.Models.DoEko.Survey
@using DoEko.Models.Identity
@{
    ViewData["Title"] = "Centralne Ogrzewanie | Pompa Ciepła";
}

<!-- Navigation -->
@if (User.IsInRole(Roles.Inspector))
{
    <ol class="breadcrumb">
        <li><a asp-action="Index" asp-controller="Home"><span class="glyphicon glyphicon-home"></span></a></li>
        <li><a asp-action="List" asp-controller="Investments">Moje inspekcje</a></li>
        <li><a asp-action="Details" asp-controller="Investments">Inwestycja @Model.InvestmentAddress.SingleLine</a></li>
        <li class="active"></li>
    </ol>
}
else
{
    <ol class="breadcrumb">
        <li><a asp-action="Index" asp-controller="Home"><span class="glyphicon glyphicon-home"></span></a></li>
        <li><a asp-action="List" asp-controller="Investments">Moje inspekcje</a></li>
        <li><a asp-action="Details" asp-controller="Investments" asp-route-id="@Model.Survey.InvestmentId">Inwestycja @Model.InvestmentAddress.SingleLine</a></li>
        <li class="active">@Model.Survey.Type.DisplayName() | @Model.Survey.RSEType.DisplayName()</li>
    </ol>

    @*<ol class="breadcrumb">
        <li><a asp-action="Index" asp-controller="Home"><span class="glyphicon glyphicon-home"></span></a></li>
        <li><a asp-action="Index" asp-controller="Projects">Lista Projektów</a></li>
        <li><a asp-action="Details" asp-controller="Projects" asp-route-Id="@Model.Contract.ProjectId">Projekt @Html.DisplayFor(model => model.Contract.Project.ShortDescription)</a></li>
        <li><a asp-action="Details" asp-controller="Contracts" asp-route-Id="@Model.ContractId">Umowa @Html.DisplayFor(model => model.Contract.Number)</a></li>
        <li class="active">Inwestycja @(Model.Address.FirstLine + " " + Model.Address.SecondLine)</li>
    </ol>*@
}

<div class="row">
    <div class="col-sm-4">
        <div class="form-group-sm">
            <label class="control-label">
                <i class="fa fa-plug" aria-hidden="true"></i> OZE
            </label>
            <p class="form-control-static">
                @Model.Survey.Type.DisplayName() | @Model.Survey.RSEType.DisplayName()
            </p>
        </div>
    </div>
    <div class="col-sm-4">
        <div class="form-group-sm">
            <label class="control-label">
                <i class="fa fa-map-marker" aria-hidden="true"></i> Inwestycja
            </label>
            <p class="form-control-static">
                @Model.InvestmentAddress.SingleLine
            </p>
        </div>
    </div>
    <div class="col-sm-4">
        <div class="form-group-sm">
            <label class="control-label">
                <i class="fa fa-cogs" aria-hidden="true"></i> Status inspekcji
            </label>
            <p class="form-control-static">
                @Model.Survey.Status.DisplayName()
            </p>
        </div>
    </div>
</div>
<hr />
<nav>
    <ul id="stepList" data-step-current="11" class="nav nav-tabs nav-justified">
        @{ int ownerCount = Model.Owners.Count() + 11;}
        <li class="active" data-step-from="11" data-step-to="@ownerCount">
            <a href="#Step_11" data-toggle="tab">Inwestycja<span class="pull-right glyphicon glyphicon-chevron-right"></span></a>
        </li>
        <li data-step-from="21" data-step-to="26">
            <a href="#Step_21" data-toggle="tab">Opis Budynku<span class="pull-right glyphicon glyphicon-chevron-right"></span></a>
        </li>
        <li data-step-from="31" data-step-to="31">
            <a href="#Step_31" data-toggle="tab">Opis Działki<span class="pull-right glyphicon glyphicon-chevron-right"></span></a>
        </li>
        <li data-step-from="41" data-step-to="42">
            <a href="#Step_41" data-toggle="tab">Audyt Energetyczny<span class="pull-right glyphicon glyphicon-chevron-right"></span></a>
        </li>
        <li data-step-from="51" data-step-to="51">
            <a href="#Step_51" data-toggle="tab">Zdjęcia</a>
        </li>
    </ul>
</nav>
<br />
<form asp-action="DetailsCH" enctype="multipart/form-data" id="mainForm">
    <div asp-validation-summary="All" class="text-danger"></div>
    <input type="hidden" asp-for="Survey.SurveyId" />
    <input type="hidden" asp-for="Survey.InvestmentId" />

<div class="tab-content">
    <!--Adres-->
